{
  "name": "@hunteroi/discord-server-generator",
  "version": "1.0.0",
  "description": "A framework to generate Discord guild categories, channels and roles, built with DiscordJS",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" --tab-width 4",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "example": "npm run build & node ./example/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hunteroi/discord-server-generator.git"
  },
  "keywords": [
    "discordjs",
    "guild-generator",
    "server-generator",
    "template-discord"
  ],
  "author": "HunteRoi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hunteroi/discord-server-generator/issues"
  },
  "homepage": "https://github.com/hunteroi/discord-server-generator#readme",
  "dependencies": {
    "discord.js": "^13.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.5",
    "@types/ws": "^8.2.0",
    "discord-sync-commands": "^0.3.0",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*"
  ]
}
